{
  "name": "Getting Started Example3",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting","tabs", "https://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
      "<all_urls>"
        ],  
      "js": ["./assets/jquery.min.js","./assets/html2canvas.min.js", "./assets/jquery-3.3.1.min.js", "boundrect.js"],
      "all-frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "index.html", "hello_world.html", "index.js", "http://news.google.com/", "https://code.jquery.com/jquery-latest.min.js", "https://html2canvas.hertzen.com/dist/html2canvas.min.js" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "host_permissions": [
    "https://youtube.googleapis.com/youtube/v3/search?part=snippet&maxResults=25&q=surfing&key=AIzaSyAIClo7wQnXWdTCMQq5jsmGIu2fKvnEhq4"
  ]
}